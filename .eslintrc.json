{
  "env": {
    "amd": true
  },
  "extends": [
    "standard",
    "eslint:recommended",
    "plugin:vue/recommended"
  ],
  "rules": {
    "vue/max-attributes-per-line": [
      "error",
      {
        "singleline": 5,
        "multiline": {
          "max": 5,
          "allowFirstLine": false
        }
      }
    ]
  }
}